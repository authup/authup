        "dist"
    ],
    "scripts": {
        "dev": "vite",
        "build:client": "vite build --ssrManifest .vite/ssr-manifest.json --outDir dist/client",
        "build:server": "vite build --ssr src/server.ts --outDir dist/server",
        "build": "npm run build:server && npm run build:client"
    },
